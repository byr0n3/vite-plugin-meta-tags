{
	"compilerOptions": {
		"module": "ESNext",
		"target": "es2017",
		"lib": ["DOM","ESNext"],
		"moduleResolution": "node",
		"strict": true,
		"declaration": true,
		"noUnusedLocals": true,
		"esModuleInterop": true,
		"outDir": "./dist",
		"sourceMap": false,
		"noEmitOnError": true,
		"noImplicitAny": false,
		"skipLibCheck": true
	},
	"include": [
		"./src"
	],
	"exclude": [
		"node_modules"
	]
}
